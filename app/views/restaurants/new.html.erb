<div class="container">
  <div class="row">
    <div class="col-6">
      <h1>New Restaurant</h1>
      <%= simple_form_for(@restaurant) do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.input :category %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
</div>
